<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Authors>egi</Authors>
    <Company>egi</Company>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <Product>Stats</Product>
